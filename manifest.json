{
  "manifest_version": 3,
  "name": "Fun Browser Plugin",
  "version": "1.0",
  "description": "A browser plugin with inversion, raw text display, jokes, and a surprise redirect.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
